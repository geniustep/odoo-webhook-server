<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Cron Job: Auto Archive Webhook Events -->
    <record id="cron_webhook_auto_archive" model="ir.cron">
        <field name="name">Webhook Auto Archive</field>
        <field name="model_id" ref="model_webhook_archiver"/>
        <field name="state">code</field>
        <field name="code">model.auto_archive()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
        <field name="priority">10</field>
    </record>
</odoo>
